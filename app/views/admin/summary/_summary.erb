<table class="index_table index">
  <thead>
    <tr>
      <th>API client</th>
      <th>Total Leads</th>
      <th>Is Verified Count </th>
    </tr>
  </thead>
  <tbody>
    <% @api_clients.each do |api_client| %>
      <%- api_users = ApiUser.where(api_client_id: api_client.id) %>
      <%- verified_api_users = api_users.verified %>
      <tr>
        <td><%= api_client.name %></td>
        <td><%= api_users.count %></td>
        <td><%= verified_api_users.count %></td>
      </tr>
    <% end %>
  </tbody>
</table>
