<% table_data[:cells].each do |date, by_date| %>
  <h3><%= date  %></h3>
  <table class="index_table index">
    <thead>
      <tr>
        <th></th>
        <% by_date.each do |hour, _| %>
          <th colspan="<%= table_data[:types].count %>"><%= hour %></th>
        <% end %>
      </tr>
      <tr>
        <th></th>
        <% by_date.each do |_, by_hour| %>
          <% table_data[:types].each do |type| %>
            <th><%= type %></th>
          <% end %>
        <% end %>
      </tr>
    </thead>
    <tbody>
      <% table_data[:affiliates].each do |a| %>
        <tr>
          <td>A<%= a %></td>
          <% by_date.each do |_, by_hour| %>
            <% table_data[:types].each do |type| %>
              <td><%= by_hour.dig(a, type) || 0 %></td>
            <% end %>
          <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
